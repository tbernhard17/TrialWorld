<Application x:Class="TrialWorld.AssemblyAIDiagnostic.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TrialWorld.AssemblyAIDiagnostic"
             xmlns:converters="clr-namespace:TrialWorld.AssemblyAIDiagnostic.Converters"
             Startup="Application_Startup">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <!-- Add theme resources here -->
            </ResourceDictionary.MergedDictionaries>
            
            <!-- Converters -->
            <converters:InverseBooleanConverter x:Key="InverseBooleanConverter" />
            <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
